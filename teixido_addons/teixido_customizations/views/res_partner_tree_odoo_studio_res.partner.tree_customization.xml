<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="teix_Odoo_Studio_res_partner_tree_customization" model="ir.ui.view">
            <field name="name">Odoo Studio: res.partner.tree customization</field>
            <field name="model">res.partner</field>
            <field name="type">tree</field>
            <field name="priority">160</field>
            <field name="active">True</field>
            <field name="inherit_id" ref="base.view_partner_tree"/>
            <field name="arch" type="xml">
                <data>
                  <xpath expr="//field[@name='complete_name']" position="after">
                    <field name="teix_tei" optional="show"/>
                    <field name="teix_pe" optional="show"/>
                    <field name="teix_re" optional="show"/>
                    <field name="teix_kf" optional="show"/>
                    <field name="teix_cfd" optional="show"/>
                    <field name="teix_cd" optional="show"/>
                    <field name="teix_pl" optional="show"/>
                  </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>
