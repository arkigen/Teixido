<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="teix_Odoo_Studio_crm_lead_kanban_lead_customization" model="ir.ui.view">
            <field name="name">Odoo Studio: crm.lead.kanban.lead customization</field>
            <field name="model">crm.lead</field>
            <field name="type">kanban</field>
            <field name="priority">320</field>
            <field name="active">True</field>
            <field name="inherit_id" ref="crm.crm_lead_view_kanban"/>
            <field name="arch" type="xml">
                <data>
                  <xpath expr="//field[@name='tag_ids']" position="after">
                    <field name="teix_selection_actual" display="full"/>
                    <field name="contact_name" display="full"/>
                  </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>
